{
  "name": "eslint-config-james-powell-dev",
  "version": "1.0.15",
  "description": "ESLint style by jamespowell.dev for React, Vue & vanilla JS projects",
  "main": "index.js",
  "repository": "https://github.com/Blazzike/eslint-config-james-powell-dev",
  "author": "James Powell (https://jamespowell.dev/)",
  "license": "MIT",
  "scripts": {
    "lint": "eslint **/*.js",
    "test": "yarn lint && eslint-find-rules --unused",
    "test:vue2": "eslint-find-rules --unused vue2.js",
    "test:vue": "eslint-find-rules --unused vue.js",
    "test:react": "eslint-find-rules --unused react.js",
    "test:next": "eslint-find-rules --unused next.js",
    "test:typescript": "eslint-find-rules --unused typescript/index.js",
    "fix": "yarn lint --fix",
    "publish": "yarn test"
  },
  "dependencies": {
    "@next/eslint-plugin-next": "^12.x",
    "@typescript-eslint/eslint-plugin": "^5.x",
    "@typescript-eslint/parser": "^5.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-react": "^7.x",
    "eslint-plugin-react-hooks": "^4.x",
    "eslint-plugin-vue": "^8.x"
  },
  "peerDependencies": {
    "eslint": "^8.x"
  },
  "devDependencies": {
    "eslint": "^8.16.0",
    "eslint-find-rules": "^4.1.0",
    "typescript": "^4.7.2"
  }
}
